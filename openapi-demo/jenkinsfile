pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                git 'https://github.com/matt-awad03/spring-demos'
                sh 'mvn package'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
            }
        }
    }
}
